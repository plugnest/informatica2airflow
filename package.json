{
  "name": "informatica2airflow",
  "displayName": "informatica2airflow",
  "description": "This extension is meant to generate airflow code based on informatica metadata",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "Add Connection",
        "command": "informatica2airflow.addconnection",
        "icon": {
          "dark": "resources/dark/add.svg",
          "light": "resources/light/add.svg"
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "connection-subjectarea",
          "title": "Informatica2Airflow",
          "icon": "resources/dark/squirrel.svg"
        }
      ]
    },
    "views": {
      "connection-subjectarea": [
        {
          "id": "connections",
          "name": "Connections"
        },
        {
          "id": "subject-areas",
          "name": "Subject Areas"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "informatica2airflow.addconnection",
          "when": "view == connections",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@types/oracledb": "^6.5.1",
    "oracledb": "^6.5.1"
  }
}